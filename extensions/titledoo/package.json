{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "titledoo",
  "title": "Capitalize Title",
  "description": "Capitalize your title according to The Chicago Manual of Style",
  "icon": "t.png",
  "author": "rinaldi",
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Capitalize Title",
      "description": "Capitalize your title according to The Chicago Manual of Style",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.2",
    "title": "^3.5.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/title": "^3.4.1",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
